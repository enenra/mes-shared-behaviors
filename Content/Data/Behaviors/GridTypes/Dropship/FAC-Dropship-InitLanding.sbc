<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <EntityComponents>

	<!--
	
	
	[AutopilotData:MSB-Dropship-Autopilot-1]
	[SecondaryAutopilotData:MSB-Dropship-Autopilot-2]	
	
	

	[Triggers:MSB-Dropship-Trigger-MoveToLanding]
	[Triggers:MSB-Dropship-Trigger-StartLanding]	
	
	[Triggers:MSB-Dropship-Trigger-ArrivedAtLandingSite]	

	[Triggers:MSB-Dropship-Trigger-LeaveLandingSite]	
	
	-->
	
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>MSB_Dropship_Trigger_MoveToLanding</SubtypeId>
      </Id>
      <Description>

      [RivalAI Trigger]
	  [Type:TargetNear]
	  [UseTrigger:true]
	  [TargetDistance:800]
	[Conditions:MSB-Dropship-Condition-MoveToLanding]
	  [MaxActions:1]
	  [Actions:MSB_Dropship_Action_MoveToLanding]
      </Description>
    </EntityComponent>


<!--
	[ChangeBehaviorSubclass:true]  
	[NewBehaviorSubclass:CargoShip]
	

	[AddWaypoints:true]
	[WaypointsToAdd:MSB-Dropship-Waypoint-LandingSite]

	-->
	
    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>MSB_Dropship_Condition_MoveToLanding</SubtypeId>
      </Id>
      <Description>

        [RivalAI Condition]

        [UseConditions:true]

        [IsTargetStatic:true]
		
		[CheckAllLoadedModIDs:true]
[AnyModIDsToCheck:2596208372]

		
      </Description>

    </EntityComponent>	
	
	
	
	
	
	
	
	
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>MSB_Dropship_Action_MoveToLanding</SubtypeId>
      </Id>
      <Description>

	[RivalAI Action]



	[ChangeAutopilotProfile:true]
	[AutopilotProfile:Secondary]	
      </Description>
      
    </EntityComponent>		
	
	
	
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>MSB_Dropship_Trigger_StartLanding</SubtypeId>
      </Id>
      <Description>

      [RivalAI Trigger]
	  [Type:TargetNear]
	  [UseTrigger:true]
	  [TargetDistance:400]
	  [MaxActions:1]
	  [Actions:MSB_Dropship_Action_StartLanding]
      </Description>
    </EntityComponent>


	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>MSB_Dropship_Action_StartLanding</SubtypeId>
      </Id>
      <Description>

	[RivalAI Action]

		[ChangeAutopilotProfile:true]
	[AutopilotProfile:Tertiary]  
      </Description>
      
    </EntityComponent>		
	
	
	
	 <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>MSB_Dropship_Waypoint_LandingSite</SubtypeId>
      </Id>
      <Description>

      [RivalAI Waypoint]
      
      [Waypoint:RelativeRandom]
      [RelativeEntity:Target]

      [MinDistance:20]
      [MaxDistance:100]
      [MinAltitude:1999]
      [MaxAltitude:2000]
      
      </Description>
      
    </EntityComponent>
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>MSB_Dropship_Trigger_ArrivedAtLandingSite</SubtypeId>
      </Id>
      <Description>

      [RivalAI Trigger]
	  [Type:Timer]
	  [UseTrigger:true]
	[StartsReady:true]
		[Conditions:MSB-Dropship-Condition-ArrivedAtLandingSite]

		
	  [MaxActions:1]
	  [Actions:MSB_Dropship_Action_ArrivedAtLandingSite]
      </Description>
    </EntityComponent>		
	
	
	    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>MSB_Dropship_Condition_ArrivedAtLandingSite</SubtypeId>
      </Id>
      <Description>

        [RivalAI Condition]

        [UseConditions:true]

        [AltitudeCheck:true]
        [MaxAltitude:8]

        [CheckGridSpeed:true]
        [MaxGridSpeed:15]

      </Description>

    </EntityComponent>
	
	
	
    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>MSB_Dropship_Action_ArrivedAtLandingSite</SubtypeId>
      </Id>
      <Description>

        [RivalAI Action]

        [EnableTriggers:true]
        [EnableTriggerNames:MSB_Dropship_Trigger_LeaveLandingSite]


        [ResetCooldownTimeOfTriggers:true]
        [ResetTriggerCooldownNames:MSB-Dropship-Trigger-LeaveLandingSite]
		
		
        [ChangeBehaviorSubclass:true]
        [NewBehaviorSubclass:Passive]

		[SetBooleansTrue:SpawnEncounters]
		
		
		[DebugMessage:Deploying troops]
      </Description>

    </EntityComponent>	
	
	
	
	
	
	<!-- Oi -->
	
    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>MSB_Dropship_Trigger_LeaveLandingSite</SubtypeId>
      </Id>
      <Description>

        [RivalAI Trigger]

        [UseTrigger:false]
        [Type:Timer]

        [Conditions:MSB-Dropship-Condition-LeaveLandingSite]

        [MinCooldownMs:1500]
        [MaxCooldownMs:1501]
        [StartsReady:false]

        [MaxActions:1]
        [Actions:MSB_Dropship_Action_LeaveLandingSite]

      </Description>

    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>MSB_Dropship_Condition_LeaveLandingSite</SubtypeId>
      </Id>
      <Description>

        [RivalAI Condition]

        [UseConditions:true]

        [AltitudeCheck:true]
        [MaxAltitude:8]

        [CheckGridSpeed:true]
        [MaxGridSpeed:10]

      </Description>

    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>MSB_Dropship_Action_LeaveLandingSite</SubtypeId>
      </Id>
      <Description>

        [RivalAI Action]


        [ChangeBehaviorSubclass:true]
        [NewBehaviorSubclass:Horsefly]


	[ChangeAutopilotProfile:true]
	[AutopilotProfile:Primary]	


		

      </Description>

    </EntityComponent>



	
	
	
  </EntityComponents>
</Definitions>