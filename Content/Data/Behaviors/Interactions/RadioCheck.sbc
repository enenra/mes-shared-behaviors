        [Actions:MSB_Interaction_RadioCheck_Action_Received_Neutral]
      </Description>
    </EntityComponent>
    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>MSB_Interaction_RadioCheck_Action_Received_Neutral</SubtypeId>
      </Id>
      <Description>
        [RivalAI Action]

        [//DebugMessage:RadioCheck - Received - Neutral]

        [PlayDialogueCue:true]
        [DialogueCueId:Interaction_RadioCheck_Neutral]
      </Description>
    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>MSB_Interaction_RadioCheck_Trigger_Received_Hostile</SubtypeId>
      </Id>
      <Description>
        [RivalAI Trigger]

        [Type:CommandReceived]
        [CommandReceiveCode:MSB_RadioCheck]

