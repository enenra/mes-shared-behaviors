<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <EntityComponents>
    <!--
        [Tags:SpawnGuards]

        [Triggers:MSB_DefenseSystem_Trigger_Guards]

        [Triggers:MSB_DefenseSystem_Trigger_Guards_SpawnCustomVariablesName] 1001

        [Triggers:MSB_DefenseSystem_Trigger_Guards_SpawnSpawnGroupName] 2001

        ex: MSB_DefenseSystem_Trigger_Guards_SpawnMilitaryBase 3001

        [Triggers:MSB_DefenseSystem_Trigger_Guards_SpawnDefault] 5001
        [Triggers:MSB_DefenseSystem_Trigger_Guards_AutoDisable] 10000
    -->

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>MSB_DefenseSystem_Trigger_Guards</SubtypeId>
      </Id>
      <Description>
        [RivalAI Trigger]

        [Type:Timer]

        [UseTrigger:true]
        [StartsReady:true]

        [MinCooldownMs:1800000]
        [MaxCooldownMs:1800001]

        [Conditions:MSB_Utility_State_Condition_Check_State_Combat]

        [MaxActions:-1]
        [Actions:MSB_DefenseSystem_Action_Guards]
      </Description>
    </EntityComponent>
    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>MSB_DefenseSystem_Action_Guards</SubtypeId>
      </Id>
      <Description>
        [RivalAI Action]

        [EnableTriggers:true]
        [EnableTriggerTags:SpawnGuards]

        [ResetCooldownTimeOfTriggers:true]
        [ResetTriggerCooldownTags:SpawnGuards]
      </Description>
    </EntityComponent>

    <!-- SpawnCustomVariablesName -->
    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>MSB_DefenseSystem_Trigger_Guards_SpawnCustomVariablesName</SubtypeId>
      </Id>
      <Description>
        [RivalAI Trigger]

        [Type:Timer]

        [UseTrigger:false]
        [StartsReady:false]

        [Tags:SpawnGuards]

        [MinCooldownMs:1000]
        [MaxCooldownMs:1001]

        [Conditions:MSB_DefenseSystem_Condition_Guards_SpawnCustomVariablesName]

        [MaxActions:-1]
