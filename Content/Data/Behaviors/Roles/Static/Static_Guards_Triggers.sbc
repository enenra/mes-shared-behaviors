        <TypeId>Inventory</TypeId>
        <SubtypeId>MSB_DefenseSystem_Condition_Guards_SpawnCustomVariablesName</SubtypeId>
      </Id>
      <Description>
        [RivalAI Condition]

        [UseConditions:true]
        [MatchAnyCondition:false]

        [CheckIfSpawnGroupExist:true]
        [ExistingSpawnGroupName:{Faction}_SpawnGroup_Guards_{CustomVariablesName}]
      </Description>
    </EntityComponent>
    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>MSB_DefenseSystem_Action_Guards_SpawnCustomVariablesName</SubtypeId>
      </Id>
      <Description>
        [RivalAI Action]

        [PlayDialogueCue:true]
        [DialogueCueId:LaunchGuards]

        [SpawnEncounter:true]
        [Spawner:MSB_DefenseSystem_Spawner_Guards_SpawnCustomVariablesName]
        [ForceSameFactionOwnership:true]

        [DisableTriggers:true]
        [DisableTriggerTags:SpawnGuards]
      </Description>
    </EntityComponent>
    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>MSB_DefenseSystem_Spawner_Guards_SpawnCustomVariablesName</SubtypeId>
      </Id>
      <Description>
        [RivalAI Spawn]

        [UseSpawn:true]
        [IgnoreSafetyChecks:true]
        [StartsReady:false]
        [UseRelativeSpawnPosition:false]

        [SpawningType:CustomSpawn]

        [MinDistance:250]
        [MaxDistance:2500]
        [MinAltitude:600]
        [MaxAltitude:1400]

        [MaxSpawns:-1]
        [SpawnGroups:{Faction}_SpawnGroup_Guards_{CustomVariablesName}]
      </Description>
    </EntityComponent>


