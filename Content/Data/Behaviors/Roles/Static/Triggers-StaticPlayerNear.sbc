	[MinPlayerReputation:-500]
	[MaxPlayerReputation:500]
	
	
	[UseTrigger:true]
	[StartsReady:true]
	  
	[Actions:MSB_Static_Action_GreetingFriendly]	  
      </Description>
      
    </EntityComponent>

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
